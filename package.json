{
  "name": "nautime",
  "displayName": "Nau Time Tracker",
  "description": "An easy automatic time tracker: Increases your work efficiency and assists with time management",
  "version": "1.0.9",
  "publisher": "nautime",
  "author": {
    "name": "Nau"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Education",
    "Visualization"
  ],
  "keywords": [
    "nau",
    "nau time",
    "time",
    "tracking",
    "tracker",
    "time tracker",
    "time tracking",
    "work",
    "worktime",
    "code",
    "code time",
    "codetime",
    "analytics",
    "task",
    "focus",
    "metrics",
    "productivity",
    "perfomance"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "homepage": "https://nautime.io/",
  "bugs": {
    "url": "https://github.com/KaefDevelopment/vscode-plugin/issues",
    "email": "support@nautime.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KaefDevelopment/vscode-plugin.git"
  },
  "license": "BSD-3-Clause",
  "main": "./dist/extension.js",
  "icon": "images/nau-logo.png",
  "contributes": {
    "commands": [
      {
        "command": "nau-time-tracker.status-bar-click",
        "title": "Click on Nau Time Tracker icon"
      }
    ],
    "icons": {
      "nau-logo": {
        "description": "Nau icon",
        "default": {
          "fontPath": "./icons.woff",
          "fontCharacter": "\\E900"
        }
      }
    }
  },
  "galleryBanner": {
    "theme": "dark",
    "color": "#0A0A0A"
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/uuid": "^9.0.7",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "axios": "^1.6.6",
    "os": "^0.1.2",
    "uuid": "^9.0.1"
  }
}
